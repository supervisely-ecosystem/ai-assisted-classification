{
  "name": "AI assisted classification",
  "type": "app",
  "categories": [
    "labeling"
  ],
  "description": "Use neural network in labeling interface to classify images and objects",
  "docker_image": "supervisely/base-py-sdk:6.1.86",
  "main_script": "annotation-tool/src/main.py",
  "gui_template": "annotation-tool/src/gui.html",

  "task_location": "application_sessions",
  "isolate": true,
  "headless": false,
  "integrated_into": ["image_annotation_tool"],

  "icon": "https://i.imgur.com/8UbfUjl.png",
  "icon_background": "#FFFFFF",
  "hotkeys": [
      {"hotkey": "ctrl+m", "command": "inference"}
  ],
  "instance_version": "6.4.18"
}
